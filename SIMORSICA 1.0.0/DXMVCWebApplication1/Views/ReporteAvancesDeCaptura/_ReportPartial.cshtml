@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "Report";
    settings.CallbackRouteValues = new { Controller = "ReporteAvancesDeCaptura", Action = "ReportPartial", ComboUR = ViewData["ComboUR"] };
    settings.ExportRouteValues = new { Controller = "ReporteAvancesDeCaptura", Action = "ReportPartialExport", ComboUR = ViewData["ComboUR"] };
	settings.Report = (XtraReport)Model;
	settings.ToolbarMode = DocumentViewerToolbarMode.StandardToolbar;
}).GetHtml()